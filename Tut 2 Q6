Mem_100	DCD		0x00000102
Mem_104	DCD		0xFFFEAABB
Mem_108	DCD		0x12345678
Mem_10C	DCD		0x9ABCDEF0
Mem_110	DCD		0x00000000
Mem_114	DCD		0x11223344
Mem_118	DCD		0x99887766
Mem_11C	DCD		0xFEEDDEED
START
		MOV		R0,#0x7
		MOV		R1, #0x11C
loop		LDRNE	R2,[R1,#-4]!
		STR		R2,[R1,#4]
		SUBS		R0,R0,#1
		BNE		loop
		END
		
